<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Gls.Cookbook.App.Views.MeasurementsPage"
             Title="MeasurementsPage">
    <VerticalStackLayout>
        <Button Text="Volume" CornerRadius="0" BorderWidth="0" BackgroundColor="Transparent" TextColor="Black" Command="{Binding VolumeMeasurementsCommand}" />
        <Button Text="Weight" CornerRadius="0" BorderWidth="0" BackgroundColor="Transparent" TextColor="Black" Command="{Binding WeightMeasurementsCommand}" />
    </VerticalStackLayout>
</ContentPage>