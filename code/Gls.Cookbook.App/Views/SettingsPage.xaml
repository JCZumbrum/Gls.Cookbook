<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Gls.Cookbook.App.Views.SettingsPage"
             Title="SettingsPage">
    <VerticalStackLayout>
        <Button Text="Measurements" CornerRadius="0" BorderWidth="0" BackgroundColor="Transparent" TextColor="Black" Command="MeasurementsCommand" />
        <Button Text="Ingredients" CornerRadius="0" BorderWidth="0" BackgroundColor="Transparent" TextColor="Black" Command="IngredientsCommand" />
        <Button Text="Conversions" CornerRadius="0" BorderWidth="0" BackgroundColor="Transparent" TextColor="Black" Command="ConversionsCommand" />
    </VerticalStackLayout>
</ContentPage>